{
    "name": "branch-time-tracker",
    "displayName": "Branch Time Tracker",
    "description": "Track your coding time per git branch",
    "version": "0.3.3",
    "publisher": "davidcacote",
    "repository": {
        "type": "git",
        "url": "https://github.com/yourusername/branch-time-tracker.git"
    },
    "engines": {
        "vscode": "^1.60.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "icon": "media/icon124.png",
    "main": "./builds/out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "branch-time-tracker.showStats",
                "title": "Show Branch Time Stats"
            },
            {
                "command": "branch-time-tracker.togglePause",
                "title": "Toggle Branch Time Tracking (Pause/Resume)"
            },
            {
                "command": "branch-time-tracker.exportData",
                "title": "Export Branch Time Data"
            },
            {
                "command": "branch-time-tracker.importData",
                "title": "Import Branch Time Data"
            }
        ],
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "branch-time-tracker",
                    "title": "Branch Time",
                    "icon": "media/icon124.png"
                }
            ]
        },
        "views": {
            "explorer": [
                {
                    "id": "branch-time-view",
                    "name": "Branch Time"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "vscode:package": "vsce package --out builds/branch-time-tracker-v0.3.3-$(date +%Y%m%d-%H%M%S).vsix",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile",
        "test": "jest",
        "test:unit": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "test:vscode": "node ./builds/out/test/runTest.js",
        "lint": "tslint src/**/*.ts",
        "lint:fix": "tslint src/**/*.ts --fix",
        "analyze": "npm run lint && npm run test:coverage",
        "build:clean": "rm -rf builds/out/ && npm run compile",
        "build:full": "npm run build:clean && npm run vscode:package"
    },
    "devDependencies": {
        "@types/glob": "^7.1.3",
        "@types/jest": "^29.5.0",
        "@types/mocha": "^10.0.0",
        "@types/node": "^16.18.126",
        "@types/vscode": "^1.60.0",
        "@vscode/test-electron": "^2.3.0",
        "@vscode/vsce": "^3.6.0",
        "jest": "^29.5.0",
        "mocha": "^10.0.0",
        "ts-jest": "^29.1.0",
        "ts-node": "^10.9.1",
        "tslint": "^6.1.0",
        "typescript": "^4.5.0"
    }
}