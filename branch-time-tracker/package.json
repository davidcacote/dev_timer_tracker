{
  "name": "branch-time-tracker",
  "displayName": "Branch Time Tracker",
  "description": "Track time spent coding on different git branches",
  "version": "0.2.1",
  "publisher": "branch-time-tracker",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "workspaceContains:.git"
  ],
  "main": "./out/extension.js",
  "icon": "media/icon.png",
  "contributes": {
    "commands": [
      {
        "command": "branch-time-tracker.showStats",
        "title": "Show Branch Time Stats"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "branch-time-tracker",
          "title": "Branch Time",
          "icon": "media/icon.png"
        }
      ]
    },
    "views": {
      "branch-time-tracker": [
        {
          "id": "branch-time-tracker.view",
          "name": "Time Tracker"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.60.0",
    "@types/node": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.39.0",
    "typescript": "^4.9.5",
    "@vscode/test-electron": "^2.3.0"
  }
}
